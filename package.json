{
  "name": "parcel-js-react-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joshwilkerson/parcel-js-react-starter"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel ./src/index.html --port 1111 --open",
    "restart": "cross-env NODE_ENV=development parcel ./src/index.html --port 1111",
    "prettier": "prettier --write 'src/**/*.js'",
    "eslint": "eslint src/ --ext .js --ext .jsx",
    "build": "cross-env NODE_ENV=production parcel build ./src/index.html --no-cache --no-minify --no-source-maps --public-url /"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "browserslist": "^4.5.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 11"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "minions.css": "^0.3.2",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-prettier": "^0.0.4",
    "prettier": "^1.10.2",
    "styled-components": "^4.3.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 11"
  ]
}
